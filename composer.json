        "symfony/config": "^3.2",
        "wiseweb-ext/site-builder-components": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://out-gitlab.wiseweb.co/wiseweb/site-builder-components.git"
        }
    ],
    "config": {
        "secure-http": false
    }
