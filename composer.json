{
    "name": "wiseweb/cms-connector-bundle",
    "license": "proprietary",
    "description": "Example of the CMS connector bundle.",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "WiseWeb",
            "email": "wiseweb@wiseweb.co"
        }
